<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="goods">
 
 
     <!-- 전체 굿즈 리스트  -->
     <select id="goodsList" resultType="goodsDto">
  		SELECT *
    	FROM GOODS
     </select>
     
     <!-- 전체 굿즈 리스트 상세 -->
     <select id="goodsDetail" parameterType="goodsDto" resultType="goodsDto">
  		SELECT GNO, GCTGY, GNAME, GPRICE, GSTOCK, GDATE, GCOUNT, GORNO, 
  		   	   GORSTATUS, GORTOTALP, MNO, GORDETAILNO, GQUT, GORPRICE
    	FROM GOODS
    	WHERE GNO = #{gNo}
     </select>
     
 
 
 
 
 
 
  </mapper>


	
